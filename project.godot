; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Swordless"
config/description="Story-rich third-person runner game with roguelite elements set in the mazy city of Maasdreniev."
config/version="0.3.1.0"
run/main_scene="res://main/main.tscn"
config/features=PackedStringArray("4.4", "Mobile")
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://globals/default_bus_layout.tres"

[autoload]

Data="*res://globals/global_data.gd"
Utilities="*res://globals/global_utilities.gd"
Settings="*res://globals/global_settings.gd"
DebugUi="*res://utils/debug/debug_ui.tscn"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=1
window/size/initial_position_type=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/csg_toolbox/plugin.cfg", "res://addons/proto_shape/plugin.cfg")

[file_customization]

folder_colors={
"res://gameplay/": "orange",
"res://globals/": "purple",
"res://hub/": "blue",
"res://levels/": "red",
"res://main/": "yellow",
"res://player/": "pink",
"res://ui/": "green"
}

[gui]

theme/custom="res://ui/res/main_theme.tres"

[importer_defaults]

scene={
&"blender/nodes/cameras": false,
&"blender/nodes/custom_properties": false,
&"blender/nodes/punctual_lights": false
}

